# baseURI: https://w3id.org/tern/ontologies/org/.shapes
# imports: http://datashapes.org/dash
# imports: https://w3id.org/tern/ontologies/org/

@prefix dash: <http://datashapes.org/dash#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix tern-org: <https://w3id.org/tern/ontologies/org/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

schema:Organization
  a sh:NodeShape ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:dissolutionDate ;
      sh:datatype xsd:date ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:foundingDate ;
      sh:datatype xsd:date ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:member ;
      sh:class prov:Agent ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:name ;
      sh:datatype rdfs:Literal ;
      sh:minCount 1 ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:parentOrganization ;
      sh:class schema:Organization ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:subOrganization ;
      sh:class schema:Organization ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:taxID ;
      sh:datatype rdfs:Literal ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path tern-org:agorID ;
      sh:datatype rdfs:Literal ;
    ] ;
.
schema:Person
  a sh:NodeShape ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:affiliation ;
      sh:class schema:Organization ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:honorificPrefix ;
      sh:datatype rdfs:Literal ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:jobTitle ;
      sh:datatype rdfs:Literal ;
    ] ;
.
schema:Thing
  a sh:NodeShape ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:alternateName ;
      sh:datatype rdfs:Literal ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:description ;
      sh:datatype rdfs:Literal ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:identifier ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:image ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:name ;
      sh:datatype rdfs:Literal ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:sameAs ;
      sh:datatype xsd:anyURI ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:url ;
      sh:datatype xsd:anyURI ;
    ] ;
.
org:ChangeEvent
  a sh:NodeShape ;
  sh:property [
      a sh:PropertyShape ;
      sh:path org:originalOrganization ;
      sh:class org:Organization ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path org:resultingOrganization ;
      sh:class org:Organization ;
    ] ;
.
prov:Agent
  a sh:NodeShape ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:address ;
      sh:datatype xsd:anyURI ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:email ;
      sh:datatype rdfs:Literal ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:memberOf ;
      sh:class schema:Organization ;
    ] ;
  sh:property [
      a sh:PropertyShape ;
      sh:path schema:telephone ;
      sh:datatype rdfs:Literal ;
    ] ;
.
<https://w3id.org/tern/ontologies/org/.shapes>
  a owl:Ontology ;
  owl:imports <http://datashapes.org/dash> ;
  owl:imports tern-org: ;
.
